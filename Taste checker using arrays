import java.util.Scanner;

public class assignment{

    public static String average(int[]choice){

        int sum = 0;
        for (int i=0 ; i<choice.length ; i++){
            sum += choice[i];
        }

        if (sum/choice.length >0 && sum/choice.length < 5){
            return "aweful taste" ;
        }else{
            return "good taste";
        }

    }
    public static void main(String []args){
        Scanner input = new Scanner(System.in);

        int[] choice = new int[40];

        for(int i = 0; i<choice.length; i++){
            System.out.println("Enter the choice of Customer "+ (i+1));
            choice[i] = input.nextInt();
        }

        System.out.print("Average review : "+average(choice));
        input.close();
    }
}
